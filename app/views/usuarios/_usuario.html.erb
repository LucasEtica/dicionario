<div id="<%= dom_id usuario %>">
  <p>
    <strong>Nome:</strong>
    <%= usuario.nome %>
  </p>

  <p>
    <strong>Login:</strong>
    <%= usuario.login %>
  </p>

  <p>
    <strong>Senha:</strong>
    <%= usuario.senha %>
  </p>

  <p>
    <% if usuario.profile_picture.attached? %>
      <%= image_tag usuario.profile_picture %>
    <% end %>
  </p>

</div>
